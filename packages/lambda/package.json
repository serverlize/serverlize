{
  "name": "@serverlize/lambda",
  "description": "Serverlize Lambda modules",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/lambda.umd.js",
  "module": "dist/lambda.es5.js",
  "typings": "dist/types/lambda.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Serverlize",
    "url": "https://github.com/serverlize"
  },
  "contributors": [
    "GitHub (https://github.com/serverlize/serverlize/graphs/contributors)"
  ],
  "homepage": "https://serverlize.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/serverlize/serverlize.git"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "precommit": "$(yarn bin)/lint-staged",
    "lint": "tslint -p tsconfig.json -t codeFrame 'src/**/*.ts' -e 'src/**/*.spec.ts'",
    "prebuild": "rimraf coverage/ dist/",
    "build:tsc": "tsc --module commonjs",
    "build:rollup": "rollup -c",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build": "yarn build:tsc && yarn build:rollup",
    "start": "rollup -c -w",
    "test": "jest",
    "test:prod": "yarn test --coverage --no-cache"
  },
  "peerDependencies": {
    "@middy/core": ">=1.0.0-alpha.11",
    "@serverlize/cdk": ">=0.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@aws-cdk/cdk": "^0.17.0",
    "@middy/core": "^1.0.0-alpha.11",
    "@serverlize/cdk": "^0.0.0",
    "@types/aws-lambda": "^8",
    "@types/execa": "^0.9.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10",
    "execa": "^1",
    "jest": "^23",
    "lint-staged": "^10.2.6",
    "npm-package-json-lint": "^3",
    "prettier": "^1",
    "rimraf": "^2",
    "rollup": "^0.66.0",
    "rollup-plugin-commonjs": "^9",
    "rollup-plugin-json": "^3",
    "rollup-plugin-node-resolve": "^3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "ts-jest": "^23.10.2",
    "ts-node": "^7",
    "tslib": "^1",
    "tslint": "^5",
    "tslint-config-esg": "github:EndemolShineGroup/tslint-config-esg",
    "tslint-config-prettier": "^1",
    "typedoc": "^0.13.0",
    "typescript": "^3"
  }
}
