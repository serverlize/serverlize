{
  "name": "@serverlize/db",
  "description": "Serverlize Database modules",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/db.umd.js",
  "module": "dist/db.es5.js",
  "typings": "dist/types/db.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Serverlize",
    "url": "https://github.com/serverlize"
  },
  "contributors": [
    "GitHub (https://github.com/serverlize/serverlize/graphs/contributors)"
  ],
  "homepage": "https://serverlize.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/serverlize/serverlize.git"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "precommit": "$(yarn bin)/lint-staged",
    "lint": "tslint -p tsconfig.json -t codeFrame 'src/**/*.ts' -e 'src/**/*.spec.ts'",
    "prebuild": "rimraf dist",
    "build:tsc": "tsc --module commonjs",
    "build:rollup": "rollup -c",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build": "yarn build:tsc && yarn build:rollup && yarn build:docs",
    "start": "rollup -c -w",
    "test": "jest",
    "test:prod": "yarn lint && yarn test --coverage --no-cache",
    "ts": "$(yarn bin)/ts-node -r tsconfig-paths/register"
  },
  "peerDependencies": {
    "aws-sdk": ">=2.302.0"
  },
  "dependencies": {
    "collect.js": "^4.0.19",
    "lodash.find": "^4.6.0",
    "lodash.isarray": "^4.0.0",
    "lodash.pick": "^4.4.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/lodash.find": "^4.6.3",
    "@types/lodash.isarray": "^4.0.3",
    "@types/lodash.pick": "^4.4.3",
    "@types/node": "^10.5.2",
    "aws-sdk": "^2.302.0",
    "cross-env": "^5.0.1",
    "jest": "^23.0.0",
    "lint-staged": "^7.2.0",
    "lodash.camelcase": "^4.3.0",
    "npm-package-json-lint": "^3.1.0",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "ts-jest": "^23.1.2",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.4.2",
    "tslint": "^5.8.0",
    "tslint-config-esg": "github:EndemolShineGroup/tslint-config-esg",
    "tslint-config-prettier": "^1.1.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  }
}
